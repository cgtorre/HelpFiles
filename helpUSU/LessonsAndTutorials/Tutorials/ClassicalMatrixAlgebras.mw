<Group drawlabel="true" labelreference="L137">
<Input>
<Text-field layout="Normal" prompt="&gt; " size="14" style="Maple Input"><Font size="14">Conj := proc(A) 
local i, n;
n := nops(A);
[A[1], seq(-A[i], i=2..n)]
end;
</Font></Text-field>
</Input>
<Output>
<Text-field layout="Maple Output" style="2D Output"><Equation executable="false" style="2D Output">NiM+JSVDb25qR2YqNiMlIkFHNiQlImlHJSJuRzYiNiJDJD44JS0lJW5vcHNHNiM5JDckJjkkNiMiIiItJSRzZXFHNiQsJCY5JDYjOCQhIiIvOCQ7IiIjOCU2IjYiNiI=</Equation></Text-field>
</Output>
</Group>
<Group drawlabel="true" labelreference="L138">
<Input>
<Text-field layout="Normal" prompt="&gt; " size="14" style="Maple Input"><Font size="14">Mult := proc( A,B)
local n, i,a, b, c, d, k, ans1, ans2, ans, c_bar, d_bar; 
n := nops(A); 
k := n/2;
if nops(A) =1 and nops(B) =1 then return([A[1]*B[1]]) fi;
a := [seq(A[i], i = 1..k)];
b := [seq(A[i], i = k+1..n)];
c := [seq(B[i], i = 1..k)];
d := [seq(B[i], i = k+1..n)];
c_bar := Conj(c):
d_bar := Conj(d):
ans1 := Mult(a, c)- Mult(d_bar, b):
ans2 := Mult(d, a) + Mult(b, c_bar);
ans := [seq(ans1[i],i = 1..k),seq(ans2[i], i = 1..k)]; 
end:</Font></Text-field>
</Input>
</Group>
