<Section collapsed="false" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Heading 3" layout="Heading 3">Solution</Text-field></Title>
<Text-field style="Normal" layout="Normal"></Text-field>
<Group labelreference="L163" drawlabel="true">
<Input>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">RetractingSpace := proc(omega0)</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">local omega, A, X, n, vars, alpha, Eq, B, N, p, C, c, i;</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">omega := evalDG(omega0);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">A := Tools:-DGinfo(&quot;FrameBaseVectors&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">n := Tools:-DGinfo(&quot;FrameBaseDimension&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">vars := [`tools/_Qn`(_a,[],n)];</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">X := DGzip(vars, A,&quot;plus&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">alpha := Hook(X, omega);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">Eq := Tools:-DGinfo(alpha, &quot;CoefficientSet&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">B := LinearAlgebra:-GenerateMatrix(Eq, vars)[1];</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">N := LinearAlgebra:-NullSpace(B);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">p := nops(N);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">C := Array(1 .. p);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">for i to p do</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">c := convert(N[i], list);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">C[i] := DGzip(c, A, &quot;plus&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">od;</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">convert(C, list);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">end:</Font></Text-field>
</Input>
</Group>
<Group labelreference="L164" drawlabel="true">
<Input>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"></Text-field>
</Input>
</Group>
<Group labelreference="L165" drawlabel="true">
<Input>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">DecomposeForm := proc(omega0)</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">local A, omega, n, p, q, B, C, alpha, Eq, soln, newC1, k;</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">omega := evalDG(omega0);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">n := Tools:-DGinfo(&quot;FrameBaseDimension&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">p := Tools:-DGinfo(omega, &quot;FormDegree&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">A := RetractingSpace(omega);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">q := nops(A);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">if q &lt;&gt; n - p then return [] fi;</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">B := Tools:-DGinfo(&quot;FrameBaseForms&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">C := Annihilator(A, B);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">alpha := k &amp;mult DGzip(C, &quot;wedge&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">Eq := Tools:-DGinfo(omega0 &amp;minus alpha, &quot;CoefficientSet&quot;);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">soln:= solve(Eq, {k});</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">newC1 := eval(k &amp;mult C[1], soln);</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">[newC1, op(C[2 .. -1])];</Font></Text-field>
<Text-field prompt="&gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">end:</Font></Text-field>
</Input>
</Group>
<Text-field style="Normal" layout="Normal"></Text-field>
