<Section collapsed="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Heading 3" layout="Heading 3">Solution</Text-field></Title>
<Text-field style="Normal" layout="Normal"></Text-field>
<Text-field style="Text" size="14" bold="true" layout="Normal"><Font bold="true" size="14">Part [i]</Font></Text-field>
<Group labelreference="L138" drawlabel="true">
<Input>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">PullbackDefinition := proc(phi, alpha, X, Y, pt)</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">local a, a1, a2, a3, a4, b, b1, b2, b3, b4, c;</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">a1 := Pullback(phi, alpha);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">a2 := eval(a1, pt);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">a3 := eval(X, pt);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">a4 := eval(Y, pt);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">a := Hook([a3, a4], a2);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">b1 := Pushforward(phi, a3, pt);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">b2 := Pushforward(phi, a4, pt);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">b3 := ApplyTransformation(phi, pt);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">b4 := eval(alpha, b3);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">b := Hook([b1, b2], b4);</Font></Text-field>
<Text-field prompt="N &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">a - b</Font></Text-field>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"><Font size="14">end:</Font></Text-field>
</Input>
</Group>
<Group labelreference="L139" drawlabel="true">
<Input>
<Text-field prompt="M &gt; " style="Maple Input" size="14" layout="Normal"></Text-field>
</Input>
</Group>
