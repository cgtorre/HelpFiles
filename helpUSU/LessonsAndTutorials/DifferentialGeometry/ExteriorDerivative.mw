<Section EssayAnswerColumns="60" EssayAnswerRows="5" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" collapsed="true"><Title>
<Text-field layout="Heading 3" style="Heading 3">Solution</Text-field></Title>
<Text-field layout="Normal" style="Normal"></Text-field>
<Text-field background="[255,255,255]" bold="true" executable="false" family="Times New Roman" font_style_name="Text" foreground="[0,0,0]" italic="false" layout="Normal" opaque="false" placeholder="false" readonly="false" selection-placeholder="false" size="14" style="Text" subscript="false" superscript="false" underline="false">Part [i]</Text-field>
<Group drawlabel="true" labelreference="L119">
<Input>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">ExteriorDerivativeDefinition1 := proc(omega, X, Y)</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">local a, a1, b, b1, b2, b3;</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">a1 := ExteriorDerivative(omega):</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">a := Hook([X, Y], a1);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b1 := LieDerivative(X, Hook(Y, omega));</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b2 := LieDerivative(Y, Hook(X, omega));</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b3 := Hook(LieBracket(X, Y), omega);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b := evalDG(b1 - b2 - b3);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">evalDG(a - b);</Font></Text-field>
<Text-field layout="Normal" prompt="M &gt; " size="14" style="Maple Input"><Font size="14">end:</Font></Text-field>
</Input>
</Group>
<Text-field layout="Normal" style="Normal"></Text-field>
<Text-field layout="Normal" style="Normal">Test ExteriorDerivativeDefinition1.</Text-field>
<Group drawlabel="true" labelreference="L120">
<Input>
<Text-field layout="Normal" prompt="M &gt; " size="14" style="Maple Input"><Font size="14">ExteriorDerivativeDefinition1(alpha1, X1, X2);</Font></Text-field>
</Input>
<Output>
<Text-field layout="Maple Output" size="14" style="2D Output"><Equation executable="false" style="2D Output">NiMiIiE=</Equation></Text-field>
</Output>
</Group>
<Group drawlabel="true" labelreference="L121">
<Input>
<Text-field layout="Normal" prompt="M &gt; " size="14" style="Maple Input"><Font size="14">ExteriorDerivativeDefinition1(alpha1, X2, X3);</Font></Text-field>
</Input>
<Output>
<Text-field layout="Maple Output" size="14" style="2D Output"><Equation executable="false" style="2D Output">NiMiIiE=</Equation></Text-field>
</Output>
</Group>
<Text-field layout="Normal" style="Normal"></Text-field>
<Text-field background="[255,255,255]" bold="true" executable="false" family="Times New Roman" font_style_name="Text" foreground="[0,0,0]" italic="false" layout="Normal" opaque="false" placeholder="false" readonly="false" selection-placeholder="false" size="14" style="Text" subscript="false" superscript="false" underline="false">Part [ii]</Text-field>
<Group drawlabel="true" labelreference="L122">
<Input>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">ExteriorDerivativeDefinition2 := proc(omega, X, Y, Z)</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">local a, a1, b1, b2, b3, b4, b5, b6, b;</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">a1 := ExteriorDerivative(omega):</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">a := Hook([X, Y, Z], a1);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b1 := LieDerivative(X, Hook([Y, Z], omega));</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b2 := LieDerivative(Y, Hook([X, Z], omega));</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b3 := LieDerivative(Z, Hook([X, Y], omega));</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b4 := Hook([LieBracket(X, Y), Z], omega);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b5 := Hook([LieBracket(X, Z), Y], omega);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b6 := Hook([LieBracket(Y, Z), X], omega);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">b := evalDG(b1 - b2 + b3 - b4 + b5 - b6);</Font></Text-field>
<Text-field layout="Normal" prompt="E3 &gt; " size="14" style="Maple Input"><Font size="14">simplify(evalDG(a - b));</Font></Text-field>
<Text-field layout="Normal" prompt="M &gt; " size="14" style="Maple Input"><Font size="14">end:</Font></Text-field>
</Input>
</Group>
